<p id="notice"><%= notice %></p>

<h1>Room Room Properties</h1>

<table>
  <thead>
    <tr>
      <th>Room</th>
      <th>Room properties</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @room_room_properties.each do |room_room_property| %>
      <tr>
        <td><%= room_room_property.room %></td>
        <td><%= room_room_property.room_properties %></td>
        <td><%= link_to 'Show', room_room_property %></td>
        <td><%= link_to 'Edit', edit_room_room_property_path(room_room_property) %></td>
        <td><%= link_to 'Destroy', room_room_property, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Room Room Property', new_room_room_property_path %>
