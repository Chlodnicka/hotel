<p id="notice"><%= notice %></p>

<h1>Room Type Of Rooms</h1>

<table>
  <thead>
    <tr>
      <th>Room</th>
      <th>Type of room</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @room_type_of_rooms.each do |room_type_of_room| %>
      <tr>
        <td><%= room_type_of_room.room %></td>
        <td><%= room_type_of_room.type_of_room %></td>
        <td><%= link_to 'Show', room_type_of_room %></td>
        <td><%= link_to 'Edit', edit_room_type_of_room_path(room_type_of_room) %></td>
        <td><%= link_to 'Destroy', room_type_of_room, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

