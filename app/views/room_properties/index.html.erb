<p id="notice"><%= notice %></p>

<h1>Room Properties</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @room_properties.each do |room_property| %>
      <tr>
        <td><%= room_property.name %></td>
        <td><%= link_to 'Show', room_property %></td>
        <td><%= link_to 'Edit', edit_room_property_path(room_property) %></td>
        <td><%= link_to 'Destroy', room_property, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Room Property', new_room_property_path %>
