<p id="notice"><%= notice %></p>

<h1>Type Of Rooms</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @type_of_rooms.each do |type_of_room| %>
      <tr>
        <td><%= type_of_room.name %></td>
        <td><%= link_to 'Show', type_of_room %></td>
        <td><%= link_to 'Edit', edit_type_of_room_path(type_of_room) %></td>
        <td><%= link_to 'Destroy', type_of_room, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Type Of Room', new_type_of_room_path %>
